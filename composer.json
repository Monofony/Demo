{
    "type": "project",
    "name": "monofony/demo",
    "description": "Monofony demo edition that is deployed to monofony.mobizel.com",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "beberlei/doctrineextensions": "^1.2",
        "eightpoints/guzzle-bundle": "^7.3",
        "mobizel/symfony-capistrano": "^0.2.0",
        "monofony/admin-pack": "^0.4.0",
        "monofony/api-pack": "^0.4.0",
        "monofony/core-pack": "^0.4.0",
        "monofony/front-pack": "^0.4.0",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/security-checker": "^5.0",
        "sylius/locale-bundle": "^1.6",
        "sylius/mailer-bundle": "^1.5",
        "sylius/taxonomy-bundle": "^1.6",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.9",
        "symfony/monolog-bundle": "^3.1",
        "symfony/polyfill-php80": "^1.15",
        "symfony/webpack-encore-bundle": "^1.7",
        "twig/extensions": "^1.5",
        "xynnn/google-tag-manager-bundle": "^2.8"
    },
    "require-dev": {
        "monofony/test-pack": "^0.4",
        "symplify/easy-coding-standard": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "doctrine/inflector": "^1.4"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    }
}
